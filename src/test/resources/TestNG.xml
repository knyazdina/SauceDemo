<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG retrysingletest.Listener Example">
    <listeners>
        <listener class-name="tests.TestListener"/>
        <listener class-name="tests.AnnotationTransformer"/>
    </listeners>
    <test name="TestNG Sample Test" preserve-order="true">
        <classes>
            <class name="tests.CartTest"/>
            <class name="tests.LoginTest"/>
            <class name="tests.CheckoutTest"/>
            <class name="tests.ProductsTest"/>
        </classes>
    </test>
</suite>